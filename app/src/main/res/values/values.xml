<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorPickerView">
        <attr format="boolean" name="enableAlpha"/>
        <attr format="boolean" name="enableBrightness"/>
        <attr format="boolean" name="onlyUpdateOnTouchEventUp"/>
    </declare-styleable>
    <style name="TopDefaultsViewColorPickerPopupAnimation">
        <item name="android:windowEnterAnimation">@anim/top_defaults_view_color_picker_popup_show</item>
        <item name="android:windowExitAnimation">@anim/top_defaults_view_color_picker_popup_hide</item>
    </style>

</resources>